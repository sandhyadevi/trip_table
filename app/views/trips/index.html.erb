<% provide(:title, 'All trips') %>
<h1>All trips </h1>


<ul class="trips">
    <% @trips.each do |trip| %>
    <tr><br>
    <td><%= trip.trip_id %></td>
    <td><%= trip.title %></td>
    <td><%= trip.user_id %></td>
    <td><%= trip.destination %></td>
    <td><%= trip.start_date %></td>
    <td><%= trip.end_date%></td>
    <td><%= trip.destination_lat%></td>
    <td><%= trip.destination_lng%></td></br>
    </tr>
    
     <% end %>
</ul>

